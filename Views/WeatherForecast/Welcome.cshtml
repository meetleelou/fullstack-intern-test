@{
    ViewData["Title"] = "Index";
}

<h2>Welcome</h2>

<p>Click to obtain 10 random names</p>


<button class="randomize">Randomize</button>

<ul>
    <li>
        <label>Results</label>
    </li>
</ul>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>



    $("button").click(function () {
        $.get("/weatherforecast/test", function (data) {
            generatedNames = data;
            console.log(generatedNames);
            $("ul").append('<li>' + generatedNames[0].name.first.toString() + ' ' + generatedNames[0].name.last.toString() + '</li>');
        });
    });
</script>
